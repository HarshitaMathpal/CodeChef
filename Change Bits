class Solution {
    static int[] changeBits(int N) {
        // code here
        String temp =Integer.toBinaryString(N);
        String str="";
        for(int i=0;i<temp.length();i++){
            if(temp.charAt(i)=='0'){
                str+="1";
            }
            else{
                str+="1";
            }
        }
        int num=Integer.parseInt(str,2);
        return new int[] {num-N,num};
    }
};
